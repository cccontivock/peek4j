#
#  peek4j.agentlauncher APPLICATION
#

Bundle-Version: 1.0.0.${tstamp}
Bundle-Description: The launcher for the Peek4J Agent.


agentClass: peek4j.agentlauncher.AgentLauncher
Agent-Class: ${agentClass}
Premain-Class: ${agentClass}
Can-Redefine-Classes: true
Can-Retransform-Classes: true

-includeresource: \
	{readme.md}

-buildpath: \
	osgi.enroute.base.api;version=2.1

-testpath: \
	osgi.enroute.junit.wrapper;version=4.12
Export-Package: peek4j.agentlauncher